<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="555.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kgitbank.megakgcoffee.Controller.Cart.CartController">
   <children>
      <HBox prefHeight="41.0" prefWidth="480.0" styleClass="cart_container_hBox" stylesheets="@../../../../Styles/cart.css">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#go_to_order_cart" prefHeight="37.0" prefWidth="55.0">
               <graphic>
                  <ImageView fitHeight="31.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../img/back.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <TableView fx:id="cart_table" layoutY="39.0" prefHeight="510.0" prefWidth="480.0">
        <columns>
            <TableColumn fx:id="order_number" prefWidth="54.0" text="ON" />
          <TableColumn fx:id="menu_name" prefWidth="120.0" text="Menu Name" />
          <TableColumn fx:id="total_items" prefWidth="74.0" text="Total Items" />
            <TableColumn fx:id="price" prefWidth="73.0" text="Price" />
            <TableColumn fx:id="delete" prefWidth="77.0" text="Delete" />
            <TableColumn fx:id="order" prefWidth="88.0" text="Order" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
